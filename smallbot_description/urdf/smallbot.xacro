	<xacro:include filename="$(find smallbot_description)/urdf/smallbot_materials.xacro" />
    <xacro:property name="clearance" value="0.035" />
	<!-- abot_base -->
	<link name="base_link">
		<!-- <inertial>
			<origin xyz="0 0 0.07" rpy="0 0 0" />
